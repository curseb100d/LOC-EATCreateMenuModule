<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".BusinessDetailActivity">

    <ScrollView android:id="@+id/business_menu_page"
        android:layout_width="wrap_content"
        android:layout_height="match_parent" >

        <RelativeLayout android:id="@+id/content_container"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <View
                android:id="@+id/_bg__business_menu_page"
                android:background="@drawable/_bg__business_menu_page_shape"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp"
                android:layout_width="1280dp"
                android:layout_height="950dp" />


            <RelativeLayout android:id="@+id/menu"
                android:layout_marginTop="217dp"
                android:layout_marginStart="0dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <View
                    android:id="@+id/rectangle_16"
                    android:layout_width="1280dp"
                    android:layout_height="800dp"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="0dp"
                    android:background="@drawable/rectangle_16_shape" />

                <TextView
                    android:id="@+id/detailFoodname"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="18dp"
                    android:gravity="center"
                    android:text="Food Name"
                    android:textColor="@color/black"
                    android:textSize="40sp"
                    android:textStyle="bold"/>

                <ImageView
                    android:id="@+id/detailImage"
                    android:layout_width="325dp"
                    android:layout_height="106dp"
                    android:layout_marginLeft="470dp"
                    android:layout_marginTop="90dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/__pngtree_vector_add_icon_4184841_1" />

                <TextView
                    android:id="@+id/detailPrice"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="200dp"
                    android:gravity="center"
                    android:text="Price"
                    android:textColor="@color/black"
                    android:textSize="30sp" />

                <TextView
                    android:id="@+id/detailType"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="250dp"
                    android:gravity="center"
                    android:text="Type"
                    android:textColor="@color/black"
                    android:textSize="30sp" />

                <com.github.clans.fab.FloatingActionMenu
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/detailType"
                    android:layout_alignParentEnd="true"
                    android:layout_marginEnd="98dp"
                    android:layout_marginBottom="-362dp"
                    app:menu_colorNormal="@color/maroon"
                    app:menu_colorPressed="@color/maroon"
                    app:menu_fab_size="mini"
                    app:menu_icon="@drawable/vector_ek2"
                    app:menu_openDirection="up"
                    app:menu_showShadow="true">

                    <com.github.clans.fab.FloatingActionButton
                        android:id="@+id/editButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/baseline_edit_24"
                        app:fab_colorNormal="@color/colorAccent"
                        app:fab_showShadow="true"
                        app:fab_size="mini" />

                    <com.github.clans.fab.FloatingActionButton
                        android:id="@+id/deleteButton"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/baseline_delete_24"
                        app:fab_colorNormal="@color/colorPrimary"
                        app:fab_showShadow="true"
                        app:fab_size="mini" />

                </com.github.clans.fab.FloatingActionMenu>

                <!--                <TextView-->
<!--                    android:layout_width="wrap_content"-->
<!--                    android:layout_height="wrap_content"-->
<!--                    android:id="@+id/detailFoodname"-->
